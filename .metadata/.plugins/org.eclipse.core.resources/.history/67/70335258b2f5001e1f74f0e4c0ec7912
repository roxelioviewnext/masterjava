package com.ejercicio.liga;

public class Jugador {
	private String nombre;
	private int edad;
	private Posicion posicion;
	private int numero;
	
	public Jugador(String nombre, int edad, Posicion posicion) {
		super();
		this.nombre = nombre;
		this.edad = edad;
		this.posicion = posicion;
	}
	
	
}
